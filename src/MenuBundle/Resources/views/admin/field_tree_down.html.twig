{% extends 'SonataAdminBundle:CRUD:base_list_field.html.twig' %}

{% block field %}

    {% spaceless %}
        {% if object.parent.children is defined %}
          {% if object.parent.children[object.parent.children|length - 1].id != object.id %}
            <a href="{{ path('page_tree_down', {'page_id': object.id}) }}">{% trans %}Вниз{% endtrans %}</a>
          {% endif %}
        {% endif %}
    {% endspaceless %}
{% endblock %}