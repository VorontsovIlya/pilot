    <style>
      @media screen and (max-width: 980px) {
        #rec{{ id }} .t228__leftcontainer {
          padding: 20px;
        }
      }

      @media screen and (max-width: 980px) {
        #rec{{ id }} .t228__imglogo {
          padding: 20px 0;
        }
      }
    </style>

    <style>
      #rec{{ id }} .t-menu__link-item {
        -webkit-transition: color 0.3s ease-in-out, opacity 0.3s ease-in-out;
        transition: color 0.3s ease-in-out, opacity 0.3s ease-in-out;
        position: relative;
      }

      #rec{{ id }} .t-menu__link-item::after {
        content: '';
        position: absolute;
        left: 0;
        top: 50%;
        width: 0%;
        border-bottom: 2px solid #0000FF;
        -webkit-transition: all 0.3s ease;
        transition: all 0.3s ease;
      }

      #rec{{ id }} .t-menu__link-item.t-active::after {
        width: 100%;
        border-bottom: 2px solid #0000FF;
      }

      #rec{{ id }} .t-menu__link-item:not(.t-active):not(.tooltipstered):hover {
        color: #ffffff !important;
      }

      #rec{{ id }} .t-menu__link-item:not(.t-active):not(.tooltipstered):hover::after {
        width: 100%;
      }

      @supports (overflow:-webkit-marquee) and (justify-content:inherit) {
        #rec{{ id }} .t-menu__link-item,
        #rec{{ id }} .t-menu__link-item.t-active {
          opacity: 1 !important;
        }
      }
    </style>

    <script type="text/javascript">
      $(document).ready(function () {
        t228_highlight();
        t228_checkAnchorLinks('{{ id }}');
      });

      $(window).resize(function () {
        t228_setWidth('{{ id }}');
      });

      $(window).load(function () {
        t228_setWidth('{{ id }}');
      });

      $(document).ready(function () {
        t228_setWidth('{{ id }}');
      });

      $(window).resize(function () {
        t228_setBg('{{ id }}');
      });

      $(document).ready(function () {
        t228_setBg('{{ id }}');
      });

      $(document).ready(function () {
        t228_changebgopacitymenu('{{ id }}');
        $(window).bind('scroll', t_throttle(function () {
          t228_changebgopacitymenu('{{ id }}')
        }, 200));
      });

      $(document).ready(function () {
        t228_createMobileMenu('{{ id }}');
      });
    </script>

