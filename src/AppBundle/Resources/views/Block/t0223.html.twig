<div id="rec{{id}}" class="r t-rec t-rec_pt_105 t-rec_pb_105" style="padding-top:50px;padding-bottom:50px; " data-record-type="251">
  <!-- T0223 -->
  <div class="t223">
    <div class="t-container">
      {% for slide in params.slides01 %}
        <div class="t223-col t-col t-col_6 t-animate" data-animate-style="zoomin" data-animate-group="yes" data-animate-order="1">
          {% media slide.video, 'big' with {'player_parameters': {'t223': 1}} %}
          <div class="t223__blocktext t-text t-text_sm" field="text">
            <div style="text-align:center;color:#362d66;" data-customstyle="yes">{{ slide.slidedescr }}</div>
          </div>
        </div>
      {% endfor %}
    </div>
  </div>
</div>