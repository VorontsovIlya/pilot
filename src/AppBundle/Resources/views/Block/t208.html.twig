        <div class="t-col t-col_6 t-prefix_6">
          <div class="t-cover__wrapper t-valign_bottom" style="height:80vh;">
            <div data-hook-content="covercontent">
              <div class="t208__wrapper">
                <div class="t208__textwrapper t-animate t-animate_started" data-animate-style="fadeinleft" style="background-color: rgba(54,48,100,1);">
                  {% if params.custstrattr01 %}  
                    <div class="t208__text t-text t-text_sm" field="text" style="color:#fae2d5;">{{ params.custstrattr01|raw }}</div>
                  {% endif %}
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
