<div id="rec{{ id }}" class="r t-rec r_showed r_anim" style=" " data-record-type="734">
  <!-- t734 -->
  <div class="t734">
    <div class="t-slds t734__witharrows" style="">
      <div class="t-container_100 t-slds__main">
        <div class="t-slds__container">
          <div class="t-slds__items-wrapper t-slds_animated-slow" data-slider-transition="500" data-slider-with-cycle="true"
            data-slider-correct-height="true" data-auto-correct-mobile-width="false" data-slider-timeout="3000">

              {% set i = 1 %}
              {% for slide in params.slides01 %}
                {% set class = '' %}
                {% if i == 1 %}
                  {% set class = 't-slds__item_active' %}
                {% endif %}
                <div class="t-slds__item {{ class }}" data-slide-index="{{ i }}">
                  <div class="t734__slds-wrapper t-slds__wrapper">

                    <div class="t-cover__carrier" data-content-cover-bg="{% path slide.image, 'reference' %}"
                      style="background-image: url({% path slide.image, 'preview' %}); height: 100vh;" src=""></div>

                    <div class="t-cover__filter" style="height:100vh; background-color:#000;filter: alpha(opacity:40); KHTMLOpacity: 0.40; MozOpacity: 0.40; opacity: 0.40; "></div>
                    <div class="t734__table" style="height:100vh;">
                      <div class="t-cell t-valign_middle">
                        <div class="t-container">
                          <div class="t734__wrapper t-width t-width_8 t-margin_auto">
                            <div class="t734__textwrapper t-align_center">

                              {% if slide.slidetitle %}
                                <div class="t734__title t-title t-title_sm">
                                  <div style="color:#f7e3d6;">{{ slide.slidetitle }}</div>
                                </div>
                              {% endif %}

                              {% if slide.slidedescr %}
                                <div class="t734__descr t-descr t-descr_md">
                                  <div style="color:#f7e3d6;">{{ slide.slidedescr }}</div>
                                </div>
                              {% endif %}

                              {% if slide.button %}
                                <div class="t734__button">                                  
                                  {% set newwin = (slide.link|checkdomain) %}                                  
                                  {% if newwin == true %}
                                    {% set target = "_self" %}
                                  {% else %}
                                    {% set target = "_blank" %}
                                  {% endif %}
                                  <a href="{{ slide.link }}" target="{{ target }}" class="t-btn" style="color:#ffffff;border:0px solid #ffffff;background-color:#362d66;border-radius:10px; -moz-border-radius:10px; -webkit-border-radius:10px;">
                                    <table style="width:100%; height:100%;">
                                      <tbody>
                                        <tr><td>{{ slide.button }}{{ newwin }}</td></tr>
                                      </tbody>
                                    </table>
                                  </a>
                                </div>
                              {% endif %}
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
                {% set i = i + 1 %}
              {% endfor %}

          </div>
        </div>
        <div class="t-slds__arrow_container ">
          <style type="text/css">
            #rec{{ id }} .t-slds__arrow_wrapper:hover .t-slds__arrow-withbg {
              border-color: #ffffff !important;
              background-color: rgba(254, 79, 118, 1) !important;
            }
          </style>
          <div class="t-slds__arrow_wrapper t-slds__arrow_wrapper-left" data-slide-direction="left" style="height: 965px;">
            <div class="t-slds__arrow t-slds__arrow-left t-slds__arrow-withbg" style="width: 30px; height: 30px;background-color: rgba(54,45,102,1);">
              <div class="t-slds__arrow_body t-slds__arrow_body-left" style="width: 7px;">
                <svg style="display: block" viewBox="0 0 7.3 13" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                  <style type="text/css">
                    #rec{{ id }} .t-slds__arrow_wrapper polyline {
                      -webkit-transition: stroke ease-in-out .2s;
                      -moz-transition: stroke ease-in-out .2s;
                      -o-transition: stroke ease-in-out .2s;
                      transition: stroke ease-in-out .2s;
                    }

                    #rec{{ id }} .t-slds__arrow_wrapper:hover polyline {
                      stroke: #ffffff !important;
                    }
                  </style>
                  <desc>Left</desc>
                  <polyline fill="none" stroke="#ffffff" stroke-linejoin="butt" stroke-linecap="butt" stroke-width="1" points="0.5,0.5 6.5,6.5 0.5,12.5"></polyline>
                </svg>
              </div>
            </div>
          </div>
          <div class="t-slds__arrow_wrapper t-slds__arrow_wrapper-right" data-slide-direction="right" style="height: 965px;">
            <div class="t-slds__arrow t-slds__arrow-right t-slds__arrow-withbg" style="width: 30px; height: 30px;background-color: rgba(54,45,102,1);">
              <div class="t-slds__arrow_body t-slds__arrow_body-right" style="width: 7px;">
                <svg style="display: block" viewBox="0 0 7.3 13" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                  <desc>Right</desc>
                  <polyline fill="none" stroke="#ffffff" stroke-linejoin="butt" stroke-linecap="butt" stroke-width="1" points="0.5,0.5 6.5,6.5 0.5,12.5"></polyline>
                </svg>
              </div>
            </div>
          </div>
        </div>
        <div class="t-slds__bullet_wrapper">
          {% set i = 1 %}
          {% for slide in params.slides01 %}
            {% set class = '' %}
            {% if i == 1 %}
              {% set class = 't-slds__bullet_active' %}
            {% endif %}
            <div class="t-slds__bullet {{ class }}" data-slide-bullet-for="{{i}}">
              <div class="t-slds__bullet_body" style="width: 10px; height: 10px;background-color: transparent;border: 2px solid #ffffff;"></div>
            </div>
            {% set i = i + 1 %}
          {% endfor %}
        </div>
      </div>
    </div>
  </div>
  <script type="text/javascript">
    $(document).ready(function () {
      t_sldsInit('{{ id }}');
    });
  </script>
  <style type="text/css">
    #rec{{ id }} .t-slds__bullet_active .t-slds__bullet_body {
      background-color: #ffffff !important;
    }

    #rec{{ id }} .t-slds__bullet:hover .t-slds__bullet_body {
      background-color: #ffffff !important;
    }
  </style>
  <style>
    #rec{{ id }} .t-btn:not(.t-animate_no-hover):hover {
      background-color: #fe4f76 !important;
    }

    #rec{{ id }} .t-btn:not(.t-animate_no-hover) {
      -webkit-transition: background-color 0.2s ease-in-out, color 0.2s ease-in-out, border-color 0.2s ease-in-out, box-shadow 0.2s ease-in-out;
      transition: background-color 0.2s ease-in-out, color 0.2s ease-in-out, border-color 0.2s ease-in-out, box-shadow 0.2s ease-in-out;
    }
  </style>
</div>