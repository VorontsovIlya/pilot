<body style="margin: 0px;" data-lang="RU" data-country="RU">
  <!--allrecords-->
  <div id="allrecords" class="t-records ui-sortable" data-hook="blocks-collection-content-node"
    data-blocks-animationoff="yes">
    {% for key, block in blocks %}      
      {% set news = "" %}
      {% if (block.news is defined) %}        
        {% set news = block.news %}            
      {% endif %}
      {% if ((key != 'form') and (key != 'news') and (key != 'artist')) %}
        {% set file = ['AppBundle:Block:', block.file, '.html.twig'] | join %}        
        {% set id = ['id_', key] | join  %}        
        {% if block.params %}
          {% include file with {'id': id, 'params': block.params, 'news': news } %}
        {% endif %}
      {% endif %}
    {% endfor %}
  </div>
  <!--/allrecords-->
